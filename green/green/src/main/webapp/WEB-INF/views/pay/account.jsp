<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<meta name="viewport" content="width=device-width, initial-scale=1.0">
<link rel="stylesheet" href="../../../resources/styles/account.css">
<title>계좌 결제</title>
</head>
<body>
<div class="container">
    <div class="title">계좌 결제</div>
    <div class="name">
      <p>이름</p>
      <input type="text" class="goname"></input>
    </div>
    <div class="tel">
      <p>전화번호</p>
      <input type="text" class="gotel"></input>
    </div>
    <div class="email">
      <p>이메일 주소</p>
      <input type="text" class="gomail"></input>
    </div>
    <div class="month">
      <p>참여 달</p>
      <div class="dojun_month">
        <input type="text" class="startd"></input>
        <p class="giho">~</p>
        <input type="text" class="endd"></input>
      </div>
    </div>

    <div class="gogek">
      <p class="gotext">
        <span class="gogekname">김미미 </span>님의 도전 정보
      </p>
    </div>

    <div class="challenge">
      <p class="count">
        <span class="gogekname">도전권 </span>
        3회</p>
    </div>

    <div class="price">
      <p class="chprice">
        <span class="gogekname">가격 </span>
        ooo원</p>
    </div>
    <div class="btn">
      <button class="nextBtn" style="border: none">다음</button>
    </div>
  </div>
</body>
</html>